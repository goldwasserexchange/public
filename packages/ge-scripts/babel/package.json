{
  "name": "@goldwasserexchange/babel",
  "version": "6.3.2",
  "description": "A common script to preconfigure and use babel",
  "homepage": "https://github.com/goldwasserexchange/public",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/goldwasserexchange/public.git"
  },
  "license": "MIT",
  "author": "Hans Scheuren",
  "main": "index.js",
  "scripts": {
    "lint": "yarn lint:pkg && yarn lint:js",
    "lint:js": "eslint . --ignore-path .gitignore",
    "lint:pkg": "npmPkgJsonLint .",
    "precommit": "lint-staged",
    "sort": "sort-package-json"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "yarn lint:js"
    ],
    "package.json": [
      "npmPkgJsonLint"
    ]
  },
  "eslintConfig": {
    "extends": "@goldwasserexchange"
  },
  "dependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.8",
    "@goldwasserexchange/babel-preset": "^6.3.2",
    "@goldwasserexchange/read-pkg-up-helpers": "^6.3.0",
    "cross-env": "^7.0.3",
    "cross-spawn": "^7.0.3",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@goldwasserexchange/eslint-config": "^6.3.2",
    "@goldwasserexchange/npm-package-json-lint-config": "^6.3.0",
    "eslint": "^8.9.0",
    "lint-staged": "^10.5.4",
    "npm-package-json-lint": "^4.6.0",
    "sort-package-json": "^1.49.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "npmpackagejsonlint": {
    "extends": "@goldwasserexchange/npm-package-json-lint-config"
  }
}
