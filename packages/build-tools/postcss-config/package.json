{
  "name": "@goldwasserexchange/postcss-config",
  "version": "6.2.3",
  "description": "The postcss configuration for Goldwasser Exchange",
  "homepage": "https://github.com/goldwasserexchange/public",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/goldwasserexchange/public.git"
  },
  "license": "MIT",
  "author": "Hans Scheuren",
  "main": "index.js",
  "scripts": {
    "lint": "yarn lint:pkg && yarn lint:js",
    "lint:js": "ge-scripts lint-js",
    "lint:pkg": "npmPkgJsonLint .",
    "precommit": "lint-staged",
    "sort": "sort-package-json"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "yarn lint:js"
    ],
    "package.json": [
      "npmPkgJsonLint"
    ]
  },
  "eslintConfig": {
    "extends": "@goldwasserexchange"
  },
  "dependencies": {
    "@goldwasserexchange/browserslist-config": "^6.2.3",
    "@goldwasserexchange/media-queries": "^6.2.3",
    "@goldwasserexchange/style-vars": "^6.2.3",
    "lost": "^8.3.1",
    "postcss-focus": "^4.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-reporter": "^6.0.1"
  },
  "devDependencies": {
    "@goldwasserexchange/eslint-config": "^6.2.3",
    "@goldwasserexchange/npm-package-json-lint-config": "^6.2.3",
    "ge-scripts": "^6.2.3",
    "lint-staged": "^10.0.10",
    "npm-package-json-lint": "^4.6.0",
    "sort-package-json": "^1.39.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "npmpackagejsonlint": {
    "extends": "@goldwasserexchange/npm-package-json-lint-config"
  }
}
